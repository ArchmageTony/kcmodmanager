{
  "name": "kcmodmanager",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "electron . --debug-brk=9223",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --ignore=\"vscode|modconfig|extract|instruction\"",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --ignore=\"vscode|modconfig|extract|instruction\" --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"KCModManager\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds --ignore=\"vscode|modconfig|extract|instruction\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jszip": "^3.1.5",
    "marked": "^0.5.0"
  },
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-packager": "^12.1.0"
  }
}
